{
  "Serilog": {
    "Using": ["Serilog.Sinks.Console", "Serilog.Sinks.File", "Serilog.Sinks.Graylog"],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:dd/MM/yy HH:mm:ss,fff} [{Level:u3}] {Message}{NewLine:1}"
        }
      },
      {
        "Name": "Graylog",
        "Args": {
          "hostnameOrAddress": "100.70.254.29",
          "port": 12201,
          "transportType": "Udp",
          "facility": "WorkspaceService",
          "minimumLogEventLevel": "Information"
        }
      },
      {
        "Name": "Logger",
        "Args": {
          "configureLogger": {
            "Filter": [
              {
                "Name": "ByIncludingOnly",
                "Args": {
                  "expression": "@Level = 'Information'"
                }
              }
            ],
            "WriteTo": [
              {
                "Name": "File",
                "Args": {
                  "path": "logs/log-.log",
                  "rollingInterval": "Day",
                  "rollOnFileSizeLimit": true
                }
              }
            ]
          }
        }
      }
    ],
    "Enrich": [
      "WithMachineName",
      "WithThreadId",
      "WithEnvironmentName"
    ],
    "Properties": {
      "Application": "WorkspaceService"
    }
  },
  "Jaeger": {
    "AgentHost": "100.70.254.29",
    "AgentPort": 6831,
    "ServiceName": "WorkspaceService"
  },
  "Graylog": {
    "Host": "100.70.254.29",
    "Port": 12201
  },
  "Vault": {
    "MountPath": "secret",
    "SecretPath": "WorkspaceService",
    "Token": "root",
    "Address": "http://100.70.254.29:8525"
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:5001"
      },
      "Grpc": {
        "Url": "https://0.0.0.0:50051"
      }
    }
  },
  "AllowedHosts": "*",
  "IdentityServiceUrl": "https://100.70.254.29:50050"
}
